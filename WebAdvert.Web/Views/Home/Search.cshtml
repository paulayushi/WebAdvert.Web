@model IEnumerable<WebAdvert.Web.Models.Search.SearchViewModel>

<div>
    <div>
        @foreach (var item in Model)
        {
            <div class="border rounded border-info p-1 w-100">
                <img src="http://www.iconninja.com/files/180/701/315/search-results-icon.png"
                     style="width: 24px;height: 24px;" />
                <a class="btn-link boxClick" href="@item.Id">@item.Title</a>
            </div>
        }
    </div>
</div>